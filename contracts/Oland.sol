library StringUtils {
    /**
     * @dev Returns the length of a given string
     *
     * @param s The string to measure the length of
     * @return The length of the input string
     */
    function strLen(string memory s) internal pure returns (uint256) {
        uint256 len;
        uint256 shift = 0;
        //
        bytes memory strBytes = bytes(s);
        uint256 byteLength = strBytes.length;
        for (len = 0; shift < byteLength; len++) {
            bytes1 b = strBytes[shift];
            if (b < 0x80) {
                shift += 1;
            } else if (b < 0xE0) {
                shift += 2;
            } else if (b < 0xF0) {
                shift += 3;
            } else if (b < 0xF8) {
                shift += 4;
            } else if (b < 0xFC) {
                shift += 5;
            } else {
                shift += 6;
            }
        }
        return len;
    }

    function isASCII(string memory s) internal pure returns (bool) {
        bytes memory strBytes = bytes(s);
        uint256 byteLength = strBytes.length;
        return byteLength == strLen(s);
    }

    function isLegal(string memory s) internal pure returns (bool) {
        if (!isASCII(s)) {
            return false;
        }
        bytes memory strBytes = bytes(s);
        uint256 byteLength = strBytes.length;
        for (uint256 i = 0; i <= byteLength - 1; i++) {
            if (
                !(strBytes[i] == 0x20 ||
                    (strBytes[i] >= 0x30 && strBytes[i] <= 0x39) ||
                    (strBytes[i] >= 0x41 && strBytes[i] <= 0x5a) ||
                    (strBytes[i] >= 0x61 && strBytes[i] <= 0x7a))
            ) {
                return false;
            }
        }
        return true;
    }

    function toLowerCase(string memory s)
        internal
        pure
        returns (string memory)
    {
        bytes memory strBytes = bytes(s);
        uint256 byteLength = strBytes.length;
        for (uint256 i = 0; i <= byteLength - 1; i++) {
            if (strBytes[i] >= 0x41 && strBytes[i] <= 0x5A) {
                strBytes[i] = bytes1(uint8(strBytes[i]) + 0x20);
            }
        }
        return string(strBytes);
    }

    function toUpperCase(string memory s)
        internal
        pure
        returns (string memory)
    {
        bytes memory strBytes = bytes(s);
        uint256 byteLength = strBytes.length;
        for (uint256 i = 0; i <= byteLength - 1; i++) {
            if (strBytes[i] >= 0x61 && strBytes[i] <= 0x7a) {
                strBytes[i] = bytes1(uint8(strBytes[i]) - 0x20);
            }
        }
        return string(strBytes);
    }

    function logicLen(string memory s) internal pure returns (uint256) {
        bytes memory strBytes = bytes(s);
        uint256 byteLength = strBytes.length;
        uint256 logicLength = byteLength;
        for (uint256 i = 0; i <= byteLength - 1; i++) {
            if (strBytes[i] == 0x20) {
                logicLength -= 1;
            }
        }
        return logicLength;
    }

    function filterSpace(string memory s)
        internal
        pure
        returns (string memory)
    {
        bytes memory strBytes = bytes(s);
        uint256 byteLength = strBytes.length;
        string memory newStr = "";
        bool tag = true;
        uint256 startIndex = 0;
        uint256 endIndex = byteLength - 1;

        //Locate not the space position of start
        for (uint256 i = 0; i <= byteLength - 1; i++) {
            if (strBytes[i] != 0x20) {
                startIndex = i;
                break;
            }
        }
        //Locate not the space position of end
        for (uint256 i = byteLength - 1; i >= 0; i--) {
            if (strBytes[i] != 0x20) {
                endIndex = i;
                break;
            }
        }

        for (uint256 i = startIndex; i <= endIndex; i++) {
            if (strBytes[i] != 0x20) {
                tag = true;
                newStr = string(abi.encodePacked(newStr, strBytes[i]));
            } else if (strBytes[i] == 0x20 && tag) {
                tag = false;
                newStr = string(abi.encodePacked(newStr, strBytes[i]));
            }
        }
        return newStr;
    }

    function keccak(string memory s) internal pure returns (uint256) {
        return uint256(keccak256(abi.encodePacked(s)));
    }

    function encode(string memory s, string memory s1)
        internal
        pure
        returns (string memory)
    {
        return string(abi.encodePacked(s, s1));
    }

    function bytesToUint(bytes memory b) internal pure returns (uint256) {
        uint256 number;
        for (uint256 i = 0; i < b.length; i++) {
            number = number + uint8(b[i]) * (2**(8 * (b.length - (i + 1))));
        }
        return number;
    }

    function utf8ToUnicode(string memory utf8cn)
        internal
        pure
        returns (uint16 unicodePoint)
    {
        bytes memory utf8cnBytes = bytes(utf8cn);
        uint24 utf8Int = uint24(bytesToUint(utf8cnBytes));
        uint24 first = ((utf8Int >> 4) & 0xF000) + ((utf8Int >> 2) & 0xF00);
        uint24 second = ((utf8Int >> 2) & 0xC0) + (utf8Int & 0x3F);

        unicodePoint = uint16(first + second);
    }
}

interface OlandVoucher {
    struct NFT{
        uint256 nftId;
        string  name;
        uint256 length;
        uint256 discount;
    }

    function balanceOf(address account, uint256 id)
        external
        view
        returns (uint256);

    function burn(
        address account,
        uint256 id,
        uint256 value
    ) external;

    function getNFTs() external view returns (NFT[] memory);

    function getNft(uint256 nftId) external view returns (NFT memory);

    function getNftIds() external view returns (uint256[] memory);

}

contract Oland is
    ERC721URIStorageUpgradeable,
    ERC721EnumerableUpgradeable,
    OwnableUpgradeable
{
    using AddressUpgradeable for address; //0
    using StringsUpgradeable for uint256; //1
    using SafeMathUpgradeable for uint256; //2
    using EnumerableSetUpgradeable for EnumerableSetUpgradeable.AddressSet;
    using StringUtils for string;
    using MerkleProof for bytes32[];

    string private baseURI;

    mapping(uint256 => string) public olandDetail;

    uint256 public sixSupply;
    uint256 public sevenSupply;
    uint256 public eightSupply;
    mapping(address => uint256) public sixClaimed;
    mapping(address => uint256) public sevenClaimed;
    mapping(address => uint256) public eightClaimed;

    OlandVoucher private olandVoucher =
        OlandVoucher(address(0x09A3407EBD8cD8480B646de7A618D85c677E6e42));

    uint256 public fiveSupply;
    mapping(address => uint256) public fiveClaimed;

    // variable end

    event NameRegister(
        address indexed registrant,
        string originalName,
        string name,
        uint256 cost,
        uint256 length,
        uint256 tokenId,
        uint256 time
    );

    function initialize()
        public
        initializer
    {
        __ERC721_init("oland box", "olandBox");
        __ERC721URIStorage_init();
        __ERC721Enumerable_init();
        __Ownable_init();
        baseURI = "https://metadata.oland.info/oland/";
    }

    function setVoucher(address _voucher) public onlyOwner {
        olandVoucher = OlandVoucher(_voucher);
    }

    // register without voucher
    function register(string memory oland)
        public
        payable
        returns (uint256 tokenId)
    {
        require(
            oland.isLegal(),
            "The oland name is illegal .Only letters, Spaces and numbers are allowed."
        );
        uint256 logicLen = oland.logicLen();
        require(
            logicLen >= 8 && logicLen <= 50,
            "Non-voucher users are only allow to register more than 8 character and less than 50 character"
        );

        oland = oland.filterSpace();
        string memory originalName = string(abi.encodePacked(oland));
        oland = oland.toLowerCase();
        tokenId = oland.keccak();
        uint256 price = priceByLen(logicLen);
        require(msg.value >= price, "Wrong price.");
        if (msg.value > price) {
            (payable(_msgSender())).transfer(msg.value.sub(price));
        }
        eightSupply += 1;
        eightClaimed[_msgSender()] += 1;
        _mint(_msgSender(), tokenId);
        emit NameRegister(_msgSender(),originalName ,oland,price ,logicLen, tokenId,block.timestamp);
    }

    function registerByVoucher(string memory oland, uint256 voucherId)
        public
        payable
        returns (uint256 tokenId)
    {
        OlandVoucher.NFT memory voucher = olandVoucher.getNft(voucherId); //gas saving
        require(
            olandVoucher.balanceOf(_msgSender(), voucherId) > 0,
            "Insufficient voucher"
        );
        require(
            oland.isLegal(),
            "The oland name is illegal .Only letters, Spaces and numbers are allowed."
        );
        uint256 logicLen = oland.logicLen();
        require(
            logicLen > 4 && logicLen <= 50,
            "The oland length is wrong,just allow 5 to 50"
        );

        oland = oland.filterSpace();
        string memory originalName = string(abi.encodePacked(oland));
        oland = oland.toLowerCase();
        tokenId = oland.keccak();
        require(
            logicLen == voucher.length,
            "The voucher length does not match the registration length"
        );

        uint256 price = priceByVoucher(voucherId);
        require(msg.value >= price, "Wrong price.");
        if (msg.value > price) {
            (payable(_msgSender())).transfer(msg.value.sub(price));
        }

        if (voucher.length == 5) {
            fiveSupply += 1;
            fiveClaimed[_msgSender()] += 1;
        } else if (voucher.length == 6) {
            sixSupply += 1;
            sixClaimed[_msgSender()] += 1;
        } else if (voucher.length == 7) {
            sevenSupply += 1;
            sevenClaimed[_msgSender()] += 1;
        } else if (voucher.length >= 8) {
            eightSupply += 1;
            eightClaimed[_msgSender()] += 1;
        }
        olandVoucher.burn(_msgSender(), voucherId, 1);
        _mint(_msgSender(), tokenId);
        emit NameRegister(_msgSender(),originalName ,oland,price ,logicLen, tokenId,block.timestamp);
    }

    function logicLength(string memory s) public pure returns (uint256) {
        return s.logicLen();
    }

    function setBaseURI(string memory URI) public onlyOwner {
        baseURI = URI;
    }

    function _baseURI() internal view virtual override returns (string memory) {
        return baseURI;
    }

    function getBaseURI() public view returns (string memory) {
        return _baseURI();
    }

    /**
     * @dev See {IERC721Metadata-tokenURI}.
     */
    function tokenURIByName(string memory oland)
        public
        view
        returns (string memory)
    {
        uint256 tokenId = oland.filterSpace().keccak();

        return tokenURI(tokenId);
    }

    function tokenURI(uint256 tokenId)
        public
        view
        virtual
        override(ERC721URIStorageUpgradeable, ERC721Upgradeable)
        returns (string memory)
    {
        require(
            _exists(tokenId),
            "ERC721URIStorage: URI query for nonexistent token"
        );
        string memory _tokenURI = _baseURI()
            .encode(tokenId.toString())
            .encode("/")
            .encode("data.json");
        return _tokenURI;
    }

    function priceByStr(string memory oland) public view returns (uint256) {
        uint256 len = oland.logicLen();
        return priceByLen(len);
    }

    function priceByLen(uint256 len) public view returns (uint256) {
        uint256 price;
        require(
            len >= 5,
            "The oland length is too short. The minimum length is 5 characters."
        );
        uint256 increase;
        if (len == 5) {
            price = 0.032 ether;
            increase = fiveSupply.div(10).mul(0.9 ether);
        } else if (len == 6) {
            price = 0.015 ether;
            increase = sixSupply.div(10).mul(0.5 ether);
        } else if (len == 7) {
            price = 0.006 ether;
            increase = sevenSupply.div(10).mul(0.3 ether);
        } else if (len >= 8) {
            price = 0.001 ether;
            increase = eightSupply.div(10).mul(0.1 ether);
        }
        price += increase;
        return price;
    }

    function priceByVoucher(uint256 voucherId) public view returns (uint256) {
        OlandVoucher.NFT memory voucher = olandVoucher.getNft(voucherId);
        uint256 price = priceByLen(voucher.length);
        price = price.div(100).mul(voucher.discount);
        return price;
    }

    function setMapping(uint256 tokenId, string memory value) public {
        require(_msgSender() == ownerOf(tokenId), "not the owner");
        olandDetail[tokenId] = value;
    }

    function setMappingByName(string memory oland, string memory value) public {
        uint256 tokenId = oland.filterSpace().keccak();
        require(_msgSender() == ownerOf(tokenId), "not the owner");
        olandDetail[tokenId] = value;
    }

    function convertToOlandStandard(string memory oland)
        public
        pure
        returns (string memory)
    {
        return oland.filterSpace().toLowerCase();
    }

    function exist(string memory oland) public view returns (bool) {
        return _exists(oland.filterSpace().keccak());
    }

    function ownerOfByName(string memory oland) public view returns (address) {
        return ownerOf(oland.filterSpace().keccak());
    }

    function _burn(uint256 tokenId)
        internal
        virtual
        override(ERC721URIStorageUpgradeable, ERC721Upgradeable)
    {
        super._burn(tokenId);
    }

    // Standard functions to be overridden in ERC721Enumerable
    function supportsInterface(bytes4 _interfaceId)
        public
        view
        override(ERC721Upgradeable, ERC721EnumerableUpgradeable)
        returns (bool)
    {
        return super.supportsInterface(_interfaceId);
    }

    function _beforeTokenTransfer(
        address _from,
        address _to,
        uint256 _tokenId
    ) internal override(ERC721EnumerableUpgradeable, ERC721Upgradeable) {
        super._beforeTokenTransfer(_from, _to, _tokenId);
    }

    function withdrawETH() public onlyOwner {
        uint256 balance = address(this).balance;
        require(balance > 0, "Balance should be more then zero");
        payable(address(0x4D77863Bc4958a7Da9690DC137211963c6e1fF39)).transfer(
            balance
        );
    }

    receive() external payable {}
}